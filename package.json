{
  "name": "@dip/chatkit",
  "version": "1.0.3",
  "type": "module",
  "description": "AI 对话组件 - ChatKit",
  "main": "dist/chatkit.cjs.js",
  "module": "dist/chatkit.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/chatkit.es.js",
      "require": "./dist/chatkit.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "vite build --config vite.config.lib.ts",
    "build:types": "tsc --project tsconfig.lib.json",
    "preview": "vite preview"
  },
  "keywords": [
    "chat",
    "ai",
    "react",
    "component"
  ],
  "author": "AISHU",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "echarts": "^6.0.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sql-formatter-plush": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18 || ^19.0.0",
    "@types/react-dom": "^18.3.5 || ^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  }
}
